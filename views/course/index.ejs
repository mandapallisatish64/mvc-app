<br>
<br>
<div class="container-fluid">
  <div class=row>
    <div class="col col-md-12">
      <a href="course/create" class="btn btn-primary new">
        Create a new Course
      </a>

      <h2>Recent entries</h2>
      <p>There are
        <%= courses.length %> courses.
        <br>
      </p>
      <% for (let i = courses.length-1; i >= 0; i--) {   
        course = courses[i];   %>
      <div class="panel panel-default">
        <div class="panel-heading"></div>
        <div class="text-muted pull-right"></div>
        <div class="panel-body">
          <br> <b>ID: </b><%=  course._id %>
          <br> <b>SchoolNumber: </b><%=course.SchoolNumber%>
          <br> <b>CourseNumber: </b><%=course.CourseNumber%>
          <br> <b>Name: </b><%=course.Name%>
          <br> <b>inSpring: </b><%=course.inSpring%>
          <br> <b>inSummer: </b><%=course.inSummer%>
          <br> <b>inFall: </b><%=course.inFall%>
          <hr> 
          <a href="/course/delete/<%= course._id  %>" class="btn btn-danger"> Delete </a>
          <a href="/course/details/<%= course._id %>" class="btn btn-info"> Details </a>
          <a href="/course/edit/<%= course._id %>" class="btn btn-primary"> Edit </a>
          <hr>
          <hr>
        </div>
      </div>
      <% } %>
    </div>
  </div>
</div>